// Returns the protocol for FileMaker path types.

Let(
  [
    _protocol = Case(
      _type = path.TypeFileMakerMac;			"filemac";
      _type = path.TypeFileMakerMacImage;	"imagemac";
      ( _type = path.TypeFileMakerWinNetwork )
        or ( _type = path.TypeFileMakerWin );	"filewin";
      _type = path.TypeFileMakerWinImage;		"imagewin";
      devp.Nil
    );

    _result = Case(
      IsEmpty( _protocol );	devp.Nil;
					_protocol & ":"
    )
  ];

  _result
)
